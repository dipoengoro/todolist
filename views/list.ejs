<%- include('header') -%>
<div class="box" id="heading">
  <h1><%= day %></h1>
</div>

<div class="box">
  <% for(let i = 0; i < list.length; i++) { %>
  <div class="item">
    <input type="checkbox">
    <p><%= list[i] %></p>
  </div>
  <% } %>
  <form action="/" method="post">
    <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
    <button type="submit" name="button">+</button>
  </form>
</div>
<%- include('footer') -%>
